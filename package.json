{
  "name": "exam",
  "version": "0.0.7",
  "description": "Clustered JavaScript test runner with builtin assertions",
  "dependencies": {
    "acorn": "0.6.0"
  },
  "devDependencies": {
    "coveralls": "2.11.1",
    "esprima": "1.2.2",
    "istanbul": "0.3.0",
    "zeriousify": "^0.1.10"
  },
  "keywords": [
    "exam"
  ],
  "main": "exam.js",
  "bin": "exam.js",
  "engines": {
    "node": ">=0.2.6"
  },
  "scripts": {
    "test": "./exam.js",
    "cover": "istanbul cover ./exam.js",
    "report": "open coverage/lcov-report/index.html",
    "coveralls": "istanbul cover ./exam.js --report lcovonly -- -R spec && cat ./coverage/lcov.info | coveralls && rm -rf ./coverage"
  },
  "repository": "https://github.com/lighterio/exam.git",
  "bugs": "https://github.com/lighterio/exam/issues",
  "homepage": "http://github.com/lighterio/exam",
  "author": "Sam Eubank <sameubank@gmail.com>",
  "contributors": [
    {
      "0": "Sam Eubank <sameubank@gmail.com>",
      "name": "Sam Eubank",
      "email": "sameubank@gmail.com"
    }
  ],
  "license": "MIT"
}
