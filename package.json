{
  "name": "exam",
  "version": "0.3.8",
  "description": "Clustered JavaScript test runner with builtin assertions.",
  "dependencies": {
    "acorn": "0.11.0",
    "coveralls": "2.11.2",
    "istanbul": "0.3.5"
  },
  "devDependencies": {
    "esprima": "1.2.3"
  },
  "optionalDependencies": {
    "mock-fs": "^3.2.0"
  },
  "keywords": [
    "exam",
    "test",
    "runner",
    "assertion",
    "mock",
    "library"
